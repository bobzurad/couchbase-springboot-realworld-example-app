{
  "api": {
    "admin_interface": ":4985",
    "admin_interface_authentication": true,
    "cors": {
      "headers": [
        "Content-Type",
        "Authorization"
      ],
      "login_origin": [
        "http://localhost:8100",
        "http://localhost:3000",
        "http://localhost:8080"
      ],
      "max_age": 1728000,
      "origin": [
        "http://localhost:8100",
        "http://localhost:3000",
        "http://localhost:8080"
      ]
    },
    "hide_product_version": false,
    "public_interface": ":4984"
  },
  "bootstrap": {
    "server": "couchbase://couchbase",
    "username": "Administrator",
    "password": "password",
    "use_tls_server": false,
    "server_tls_skip_verify": true
  },
  "bucket_credentials": {
    "default": {
      "username": "Administrator",
      "password": "password"
    }
  },
  "database_credentials": {
    "default": {
      "username": "Administrator",
      "password": "password"
    }
  },
  "logging": {
    "console": {
      "enabled": true,
      "log_level": "info",
      "log_keys": ["*"]
    }
  }
}
